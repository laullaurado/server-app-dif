{
    "name": "nak",
    "version": "0.3.3",
    "description": "ack and ag inspired tool written in Node. Designed to be fast.",
    "keywords": ["ag", "ack", "grep", "search"],
    "author": "Garen J. Torikian <gjtorikian@gmail.com>",
    "contributors": [{
      "name": "Kevin Sawicki",
      "email": "kevin@github.com"
    }],
    "dependencies": {
        "isbinaryfile": "2.0.x",
